//функция,которая поворачивает матрицу на 90 градусов по часовой стрелке (делал Павел)
#include "pch.h"
#include <iostream>

using namespace std;

int main() {

  int b = 0;
  int c[100][100];
  int m, n;
  cin >> n >> m;
  for (int i = 0; i < n; i++) {
    for (int j = 0; j < m; j++) {
      c[i][j] = ++b;
    }
  }
  int k = 0;
  while (k != m) {
    for (int i = n - 1; i >= 0; i--) {
      cout << c[i][k] << " ";
    }
    k++;
    cout << endl;

  }



  return 0;
}
